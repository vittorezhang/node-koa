{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": ["esnext"],
        "moduleResolution": "node",
        "outDir": "dist",
        "resolveJsonModule": true,
        "importHelpers": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "baseUrl": "./",
        "strict": true,
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@@/*": [
                "src/.umi/*"
            ]
        },
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./",
        "src/**/*"
    ]
}